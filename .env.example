# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file with real tokens to version control

# Discord Bot Token - Get from https://discord.com/developers/applications
DISCORD_BOT_TOKEN=YOUR_DISCORD_BOT_TOKEN_HERE

# OpenAI API Key - Get from https://platform.openai.com/api-keys
OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Optional: Guild ID for faster command sync (recommended for development)
# Leave empty for global commands (takes up to 1 hour to sync)
GUILD_ID=

# Optional: Specific channel for bot responses
# If not set, bot will use the first channel it has send permissions in
RESPONSE_CHANNEL=
